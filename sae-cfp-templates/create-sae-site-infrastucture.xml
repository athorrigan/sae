<config xmlns="http://tail-f.com/ns/config/1.0">
    <sae-site xmlns="http://com/cisco/nso/sae-core-fp-cfs">
        <site>SVS-RTP</site>
            <infrastructure>
                <switching>
                    <type>n9k-switch-pair</type>
                </switching>
                <compute-clusters>
                    <name>SVS-NFV-Cluster1</name>
                    <leaf-switches>
                        <name>SAE-N9K1</name>
                    </leaf-switches>
                    <leaf-switches>
                        <name>SAE-N9K2</name>
                    </leaf-switches>
                    <servers>
                        <name>SAE-CSP1</name>
                        <type>SVS-NFV-CSP2100</type>
                        <sriov-networks>
                            <name>enp129s0f0</name>
                            <connected-switch>SAE-N9K1</connected-switch>
                            <switch-interface-name>Ethernet1/1</switch-interface-name>
                        </sriov-networks>
                        <sriov-networks>
                            <name>enp129s0f1</name>
                            <connected-switch>SAE-N9K2</connected-switch>
                            <switch-interface-name>Ethernet1/1</switch-interface-name>
                        </sriov-networks>
                    </servers>
                    <servers>
                        <name>SAE-CSP2</name>
                        <type>SVS-NFV-CSP2100</type>
                        <sriov-networks>
                            <name>enp129s0f0</name>
                            <connected-switch>SAE-N9K1</connected-switch>
                            <switch-interface-name>Ethernet1/2</switch-interface-name>
                        </sriov-networks>
                        <sriov-networks>
                            <name>enp129s0f1</name>
                            <connected-switch>SAE-N9K2</connected-switch>
                            <switch-interface-name>Ethernet1/2</switch-interface-name>
                        </sriov-networks>
                    </servers>
                    <servers>
                        <name>SAE-CSP3</name>
                        <type>SVS-NFV-CSP2100</type>
                        <sriov-networks>
                            <name>enp129s0f0</name>
                            <connected-switch>SAE-N9K1</connected-switch>
                            <switch-interface-name>Ethernet1/3</switch-interface-name>
                        </sriov-networks>
                        <sriov-networks>
                            <name>enp129s0f1</name>
                            <connected-switch>SAE-N9K2</connected-switch>
                            <switch-interface-name>Ethernet1/3</switch-interface-name>
                        </sriov-networks>
                    </servers>
                    <servers>
                        <name>SAE-CSP4</name>
                        <type>SVS-NFV-CSP2100</type>
                        <sriov-networks>
                            <name>enp129s0f0</name>
                            <connected-switch>SAE-N9K1</connected-switch>
                            <switch-interface-name>Ethernet1/4</switch-interface-name>
                        </sriov-networks>
                        <sriov-networks>
                            <name>enp129s0f1</name>
                            <connected-switch>SAE-N9K2</connected-switch>
                            <switch-interface-name>Ethernet1/4</switch-interface-name>
                        </sriov-networks>
                    </servers>
                </compute-clusters>
                <vnf-manager>
                    <name>SAE-ESC</name>
                    <device-on-boarding>nso-unmanaged</device-on-boarding>
                    <vnfm-device-name>SAE-ESC</vnfm-device-name>
                </vnf-manager>
            </infrastructure>
    </sae-site>
</config>